{
  "expo": {
    "name": "pradan",
    "slug": "pradan",
    "permissions": ["CAMERA"],
    "plugins": ["expo-image-picker"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/prdanicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kavi2005.pradan",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/prdanicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kavi2005.pradan"
    },
    
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/prdanicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "API_URL": "http://192.168.1.27:5000",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f1455621-80d9-4227-8c99-44a9f66e9000"
      }
    }
  }
}
